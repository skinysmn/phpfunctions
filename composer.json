{
    "name": "levin/phpfunctions",
    "description": "Some php functions to twig filters and functions",
    "type": "bolt-extension",
    "keywords": ["php", "twig", "extension", "twig functions", "video preview", "pdf preview", "calendar", "server array", "$_server", "random array", "randomize array","str_replace", "string replace","shuffle","url decode" 
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "authors": [
        {
            "name": "Oleg Levin",
            "email": "oleg_levin@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\levin\\phpfunctions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\levin\\phpfunctions\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
        "extra": {
        "bolt-icon": "",
        "bolt-class": "Bolt\\Extension\\levin\\phpfunctions\\phpfunctionsExtension"
    }
}
